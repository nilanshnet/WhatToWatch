AA,EAAA;AACA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,MAAA,CAAA;;AACA,YACA,QAAA,CAAA,MAAA,CAAA,IAAA,6CADA,EAEA;AACA,UAAA,MAAA,CAAA,SAAA,CAAA,gBAAA;AACA;;AACA,QAAA,MAAA,CAAA,UAAA;AACA,OApBA,EAqBA,KArBA,CAqBA,UAAA,KAAA,EAAA;AACA,QAAA,KAAA,CAAA,gBAAA,CAAA;AACA,QAAA,OAAA,CAAA,KAAA,CAAA,SAAA,EAAA,KAAA;AACA,OAxBA;AAyBA,KA7FA;AA+FA,IAAA,UA/FA,wBA+FA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,MAAA,CAAA,WAAA,GAAA,IAAA;AADA;AAAA,uBAEA,IAAA,CAAA,cAAA,GAAA,IAAA,CAAA,UAAA,IAAA;AAAA,yBACA,KAAA,CAAA;AACA,oBAAA,MAAA,EAAA,KADA;AAEA,oBAAA,GAAA,EAAA,MAAA,CAAA,GAAA,CAAA,SAAA,GAAA,SAFA;AAGA,oBAAA,OAAA,EAAA;AACA,sBAAA,aAAA,EAAA,IAAA,CAAA,OAAA,CAAA;AADA,qBAHA;AAMA,oBAAA,MAAA,EAAA;AACA,sBAAA,MAAA,EAAA,MAAA,CAAA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,IAAA,CAAA;AADA;AANA,mBAAA,CAAA,CAUA,IAVA,CAUA,UAAA,QAAA,EAAA;AACA,oBAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,MAAA,CAAA,CAAA,aAAA,CAAA,EADA,CAEA;;AAAA;AACA,oBAAA,MAAA,CAAA,WAAA,GAAA,QAAA,CAAA,MAAA,CAAA,CAAA,aAAA,CAAA;AACA,oBAAA,MAAA,CAAA,IAAA,GAAA,MAAA,CAAA,WAAA,CAAA,MAAA;AACA,oBAAA,MAAA,CAAA,WAAA,GAAA,KAAA;AACA,mBAhBA,EAiBA,KAjBA,CAiBA,UAAA,KAAA,EAAA;AACA;AACA,oBAAA,OAAA,CAAA,KAAA,CAAA,SAAA,EAAA,KAAA;AACA,mBApBA,CADA;AAAA,iBAAA,CAFA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAyBA;AAxHA;AAzBA,CAAA","sourcesContent":["<template>\n  <div id=\"moviesSocial\" style=\"background-color: black\">\n    <b-row no-gutters>\n      <b-col md=\"6\" style=\"padding-left: 10%\" :key=\"reloadFriends\">\n        <h1>Friends List</h1>\n        <br />\n\n        <div v-show=\"showLoading\" id=\"loadingSocial\">\n          <Spinner />\n        </div>\n        <div class=\"container\">\n          <b-card-text>\n            <ul class=\"list group\">\n              <li\n                class=\"list-group-item d-flex justify-content-between align-items-center\"\n                id=\"rcorners2\"\n                v-for=\"friend in friendsList\"\n                :key=\"friend\"\n              >\n                <h4>{{ friend.friendname }}</h4>\n                <div>\n                  <span class=\"badge badge-primary badge-pill\">\n                    {{ friend.friendid }}</span\n                  >\n                  <b-button\n                    variant=\"danger\"\n                    style=\"margin: 10px\"\n                    class=\"btn-sm\"\n                    v-on:click=\"removeFriend(friend)\"\n                    >Remove</b-button\n                  >\n                </div>\n              </li>\n            </ul>\n          </b-card-text>\n        </div>\n      </b-col>\n      <b-col md=\"5\">\n        <div style=\"position: relative; left: 10%\">\n          <h1>Add Friends</h1>\n          <br />\n        </div>\n        <div class=\"right-container\">\n          <b-form\n            @submit=\"onSubmit\"\n            @reset=\"onReset\"\n            v-if=\"show\"\n            style=\"\n              padding-top: 10px;\n              padding-bottom: 20px;\n              justify-content: center;\n              align-items: center;\n              padding-left: 20%;\n            \"\n          >\n            <b-form-group id=\"input-group-1\" label-for=\"input-1\">\n              <b-form-input\n                id=\"input-1\"\n                v-model=\"form.email\"\n                type=\"text\"\n                placeholder=\"Enter userid of friend\"\n                required\n              ></b-form-input>\n            </b-form-group>\n            <b-button\n              type=\"submit\"\n              variant=\"success\"\n              class=\"AddFriend\"\n              style=\"margin: 10px\"\n              >Add Friend</b-button\n            >\n            <!--<b-button type=\"reset\" variant=\"danger\" class=\"RemoveFriend\" style=\"margin:10px\">Remove</b-button>-->\n          </b-form>\n        </div>\n      </b-col>\n    </b-row>\n\n    <b-modal\n      ref=\"friend_added\"\n      centered\n      hide-header\n      hide-footer\n      body-bg-variant=\"dark\"\n      body-text-variant=\"white\"\n      body-border-variant=\"dark\"\n    >\n      <div class=\"d-block text-center\" style=\"font-szie = 30px\">\n        New friend has been added.\n      </div>\n      <div class=\"d-block text-right\">\n        <b-button class=\"btn btn-success\" @click=\"hideModal('friend_added')\">\n          Ok\n        </b-button>\n      </div>\n    </b-modal>\n\n    <b-modal\n      ref=\"friend_removed\"\n      centered\n      hide-header\n      hide-footer\n      body-bg-variant=\"dark\"\n      body-text-variant=\"white\"\n      body-border-variant=\"dark\"\n    >\n      <div class=\"d-block text-center\" style=\"font-szie = 30px\">\n        Friend has been removed.\n      </div>\n      <div class=\"d-block text-right\">\n        <b-button class=\"btn btn-success\" @click=\"hideModal('friend_removed')\">\n          Ok\n        </b-button>\n      </div>\n    </b-modal>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport Spinner from \"@/components/Spinner\";\nimport { Auth } from \"aws-amplify\";\nimport { mapGetters } from \"vuex\";\nvar config = require(\"../config.js\");\nexport default {\n  data() {\n    return {\n      selectedCard: -1,\n      friendsList: [],\n      showLoading: true,\n      reloadFriends: 0,\n      size: -1,\n      form: {\n        email: \"\",\n      },\n      show: true,\n    };\n  },\n  mounted() {\n    this.loadSocial();\n  },\n  components: {\n    Spinner,\n  },\n  computed: {\n    ...mapGetters({\n      user: \"auth/user\",\n    }),\n  },\n  methods: {\n    showModal(name) {\n      this.$refs[name].show();\n    },\n    hideModal(name) {\n      this.$refs[name].hide();\n    },\n\n    onSubmit(event) {\n      event.preventDefault();\n      axios({\n        method: \"POST\",\n        url: config.api.invokeUrl + \"/friend/add\",\n        headers: {\n          Authorization: this.$store.state.auth.token,\n        },\n        data: {\n          userid: this.$store.state.auth.user.username,\n          friendid: this.form.email,\n        },\n      })\n        .then((response) => {\n          // handle success\n          console.log(response[\"data\"]);\n          if (response[\"data\"] == \"Friend added successfully.\") {\n            this.showModal(\"friend_added\");\n          }\n          // this.reloadFriends += 1;\n\n          this.loadSocial();\n        })\n        .catch(function (error) {\n          alert(\"Error Occurred\");\n          console.error(\"Error: \", error);\n        });\n    },\n\n    onReset(event) {\n      event.preventDefault();\n      // Reset our form values\n      axios({\n        method: \"POST\",\n        url: config.api.invokeUrl + \"/friend/remove\",\n        headers: {\n          Authorization: this.$store.state.auth.token,\n        },\n        data: {\n          userid: this.$store.state.auth.user.username,\n          friendid: this.form.email,\n        },\n      })\n        .then((response) => {\n          // handle success\n          console.log(response[\"data\"]);\n        })\n        .catch(function (error) {\n          alert(\"Error Occurred\");\n          console.error(\"Error: \", error);\n        });\n      this.form.email = \"\";\n      // Trick to reset/clear native browser form validation state\n      this.show = false;\n      this.$nextTick(() => {\n        this.show = true;\n      });\n    },\n    removeFriend(friend) {\n      // Reset our form values\n      axios({\n        method: \"POST\",\n        url: config.api.invokeUrl + \"/friend/remove\",\n        headers: {\n          Authorization: this.$store.state.auth.token,\n        },\n        data: {\n          userid: this.$store.state.auth.user.username,\n          friendid: friend.friendid,\n        },\n      })\n        .then((response) => {\n          // handle success\n          console.log(response[\"data\"]);\n          if (\n            response[\"data\"] == \"Removed friend from friendlist successfully\"\n          ) {\n            this.showModal(\"friend_removed\");\n          }\n          this.loadSocial();\n        })\n        .catch(function (error) {\n          alert(\"Error Occurred\");\n          console.error(\"Error: \", error);\n        });\n    },\n\n    async loadSocial() {\n      this.showLoading = true;\n      await Auth.currentSession().then((data) =>\n        axios({\n          method: \"GET\",\n          url: config.api.invokeUrl + \"/friend\",\n          headers: {\n            Authorization: data.idToken.jwtToken,\n          },\n          params: {\n            userid: this.$store.state.auth.user.username,\n          },\n        })\n          .then((response) => {\n            console.log(response[\"data\"][\"friend_list\"]);\n            // handle success\n            this.friendsList = response[\"data\"][\"friend_list\"];\n            this.size = this.friendsList.length;\n            this.showLoading = false;\n          })\n          .catch(function (error) {\n            // handle error\n            console.error(\"Error: \", error);\n          })\n      );\n    },\n  },\n};\n</script>\n<style lang=\"scss\" scoped>\n.moviesSocial {\n  position: relative;\n  left: 10px;\n  right: 10px;\n  top: 100%;\n  justify-content: center;\n  width: 70%;\n  align-items: center;\n}\n\n#rcorners2 {\n  border-radius: 8px;\n  border: 2px solid;\n  color: green;\n  background: #e4f5e4;\n}\n\nh1 {\n  align-items: center;\n  color: #cacaca;\n  font-family: Arial, Helvetica, sans-serif;\n  font-size: 22px;\n  font-weight: bold;\n  margin-bottom: 10px;\n  margin-top: 15px;\n  display: flex;\n  justify-content: center;\n}\nh4 {\n  align-items: center;\n  color: #302f2f;\n  font-family: Arial, Helvetica, sans-serif;\n  font-size: 20px;\n  font-weight: bold;\n  margin-bottom: 10px;\n  margin-top: 15px;\n  display: flex;\n  justify-content: center;\n}\n</style>"],"sourceRoot":"src/views"}]}