,QAAA,CAAA,MAAA,CAAA,CAAA,kBAAA,CAAA;AACA,oBAAA,MAAA,CAAA,IAAA,GAAA,MAAA,CAAA,eAAA,CAAA,MAAA;AACA,oBAAA,MAAA,CAAA,iBAAA,GAAA,MAAA,CAAA,aAAA,CAAA,MAAA;AACA,oBAAA,MAAA,CAAA,mBAAA,GAAA,MAAA,CAAA,eAAA,CAAA,MAAA;AACA,oBAAA,MAAA,CAAA,WAAA,GAAA,KAAA;;AACA,wBAAA,MAAA,IAAA,KAAA,EAAA;AACA,sBAAA,OAAA,CAAA,GAAA,CAAA,MAAA;AACA,sBAAA,QAAA,CAAA,MAAA;AACA;AACA,mBAzBA,EA0BA,KA1BA,CA0BA,UAAA,KAAA,EAAA;AACA;AACA,oBAAA,OAAA,CAAA,KAAA,CAAA,SAAA,EAAA,KAAA;AACA,mBA7BA,CADA;AAAA,iBAAA,CAFA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAkCA,KA1IA;AA2IA,IAAA,SA3IA,qBA2IA,IA3IA,EA2IA;AACA,WAAA,KAAA,CAAA,IAAA,EAAA,IAAA;AACA,KA7IA;AA8IA,IAAA,SA9IA,qBA8IA,IA9IA,EA8IA;AACA,WAAA,KAAA,CAAA,IAAA,EAAA,IAAA;AACA;AAhJA;AAlCA,CAAA","sourcesContent":["<template>\n  <div>\n    <div id=\"moviesSocial\">\n      <h1>Friends Are Watching </h1>\n      <div id=\"slide\">\n      <div v-show=\"showLoading\" id=\"loadingSocial\">\n        <Spinner />\n      </div>\n      <div v-if=\"size == 0\" class=\"container\">\n        Add more friends to see what they are watching\n      </div>\n\n\n               <carousel\n                :per-page=\"6\"\n                :navigate-to=\"0\"\n                :mouse-drag=\"false\"\n                :paginationEnabled=\"false\"\n                :navigationEnabled=\"true\"\n                :navigationClickTargetSize=\"6\"\n              >\n                <slide v-for=\"(item, index) in friendsWatching\" :key=\"index\">\n                  <div\n                    class=\"card\"\n                    :ref=\"`card_${index}`\"\n                    @mouseover=\"hoverCard(index)\"\n                    @mouseout=\"hoverCard(-1)\"\n                  >\n                    <img\n                      class=\"card-image\"\n                      :src=\"item.poster_path\"\n                      :class=\"{ selected: isSelected(index, 'ml_rec') }\"\n                    />\n                    <img\n                      class=\"card-image\"\n                      :class=\"{ selected: isSelected(index) }\"\n                      :src=\"item.poster_path\"\n                    />\n                    <h4 class=\"card-title\" :class=\"{ selected: isSelected(index) }\">\n                      {{ item.name }}\n                    </h4>\n                  </div>\n                </slide>\n              </carousel>\n      </div>\n    </div>\n\n    <b-modal\n      ref=\"event_declined\"\n      centered\n      hide-header\n      hide-footer\n      body-bg-variant=\"dark\"\n      body-text-variant=\"white\"\n      body-border-variant=\"dark\"\n    >\n      <div class=\"d-block text-center\" style=\"font-szie = 30px\">\n        Event has been declined\n      </div>\n      <div class=\"d-block text-right\">\n        <b-button class=\"btn btn-success\" @click=\"hideModal('event_declined')\">\n          Ok\n        </b-button>\n      </div>\n    </b-modal>\n\n    <b-modal\n      ref=\"event_created\"\n      centered\n      hide-header\n      hide-footer\n      body-bg-variant=\"dark\"\n      body-text-variant=\"white\"\n      body-border-variant=\"dark\"\n    >\n      <div class=\"d-block text-center\" style=\"font-szie = 30px\">\n        Your event has been created and invites have been sent out.\n      </div>\n      <div class=\"d-block text-right\">\n        <b-button class=\"btn btn-success\" @click=\"hideModal('event_created')\">\n          Ok\n        </b-button>\n      </div>\n    </b-modal>\n    <b-modal\n      ref=\"event_accepted\"\n      centered\n      hide-header\n      hide-footer\n      body-bg-variant=\"dark\"\n      body-text-variant=\"white\"\n      body-border-variant=\"dark\"\n    >\n      <div class=\"d-block text-center\" style=\"font-szie = 30px\">\n        Event has been accepted\n      </div>\n      <div class=\"d-block text-right\">\n        <b-button class=\"btn btn-success\" @click=\"hideModal('event_accepted')\">\n          Ok\n        </b-button>\n      </div>\n    </b-modal>\n\n    <div id=\"eventsSection\" style=\"padding-top: 4%; align-items: center\">\n      <b-row no-gutters>\n        <b-col md=\"5\" style=\"margin-left: 8%\">\n          <h1>Pending Events</h1>\n          <br />\n          <div v-if=\"sizePendingEvents == 0\">There are no pending events</div>\n          <ul class=\"list group\">\n            <li\n              id=\"rcorners2\"\n              v-for=\"event in pendingEvents\"\n              :key=\"event\"\n              class=\"list-group-item d-flex justify-content-between align-items-start\"\n            >\n              <div class=\"ms-2 me-auto\">\n                <div>\n                  <h5>{{ event.description }}</h5>\n                </div>\n                <h6>\n                  by <br />\n                  {{ event.creator }} at '{{ event.time }}'\n                </h6>\n              </div>\n              <div class=\"btn-group-vertical\">\n                <button\n                  type=\"button\"\n                  class=\"btn btn-success\"\n                  v-on:click=\"eventAccept(event)\"\n                >\n                  Accept\n                </button>\n                <button\n                  type=\"button\"\n                  class=\"btn btn-danger\"\n                  v-on:click=\"eventReject(event)\"\n                >\n                  Decline\n                </button>\n              </div>\n            </li>\n          </ul>\n        </b-col>\n        <b-col md=\"5\">\n          <h1>Confirmed Events</h1>\n          <br />\n\n          <div v-if=\"sizeConfirmedEvents == 0\">\n            <div>There are no confirmed events</div>\n          </div>\n          <ul class=\"list group\">\n            <li\n              id=\"rcorners2\"\n              v-for=\"event in confirmedEvents\"\n              :key=\"event\"\n              class=\"list-group-item d-flex justify-content-between align-items-start\"\n            >\n              <div class=\"ms-2 me-auto\">\n                <div>\n                  <h5>{{ event.description }}</h5>\n                </div>\n                <h6>\n                  by <br />\n                  {{ event.creator }} at '{{ event.time }}'\n                </h6>\n              </div>\n              <div class=\"btn-group-vertical\">\n                <a :href=\"event.url\" class=\"btn btn-success\">Join</a>\n                <button\n                  type=\"button\"\n                  class=\"btn btn-danger\"\n                  v-on:click=\"eventReject(event)\"\n                >\n                  Decline\n                </button>\n              </div>\n            </li>\n          </ul>\n        </b-col>\n      </b-row>\n    </div>\n    <div id=\"createWatchParty\" style=\"padding-top: 3%\">\n      <h1>Create Watch Party</h1>\n      <br />\n      <div class=\"CreateWP\">\n        <b-form\n          @submit=\"onSubmit\"\n          @reset=\"onReset\"\n          style=\"\n            padding-top: 10px;\n            padding-bottom: 20px;\n            justify-content: center;\n            align-items: center;\n          \"\n        >\n          <label for=\"watch-party-name\">Title</label><br />\n          <input required\n            style=\"\n              padding: 7px;\n              width: 20%;\n              align: center;\n              box-shadow: 0px 2px 4px 0px rgba(0, 0, 0, 0.5);\n            \"\n            v-model=\"form.description\"\n          />\n          <b-form-text style=\"padding-bottom: 20px\"> </b-form-text>\n\n          <label for=\"watch-party-time\">Date & Time</label><br />\n          <input required\n            style=\"\n              padding: 7px;\n              width: 20%;\n              align: center;\n              box-shadow: 0px 2px 4px 0px rgba(0, 0, 0, 0.5);\n            \"\n            v-model=\"form.time\"\n          />\n          <b-form-text style=\"padding-bottom: 20px\"> </b-form-text>\n\n          <label for=\"watch-party-emails\">Email(s)</label><br />\n          <input required\n            style=\"\n              padding: 7px;\n              width: 20%;\n              align: center;\n              box-shadow: 0px 2px 4px 0px rgba(0, 0, 0, 0.5);\n            \"\n            v-model=\"form.inviteList\"\n          />\n          <b-form-text style=\"padding-bottom: 20px\"> </b-form-text>\n\n          <label for=\"watch-party-time\">Invitation Link</label><br />\n          <input required\n            style=\"\n              padding: 7px;\n              width: 20%;\n              align: center;\n              box-shadow: 0px 2px 4px 0px rgba(0, 0, 0, 0.5);\n            \"\n            v-model=\"form.inviteUrl\"\n          />\n          <b-form-text style=\"padding-bottom: 20px\"> </b-form-text>\n\n          <b-button type=\"submit\" variant=\"success\" style=\"margin: 10px\"\n            >Submit</b-button\n          >\n          <b-button type=\"reset\" variant=\"danger\" style=\"margin: 10px\"\n            >Reset</b-button\n          >\n        </b-form>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { Carousel, Slide } from \"vue-carousel\";\nimport axios from \"axios\";\nimport Spinner from \"@/components/Spinner\";\nimport { Auth } from \"aws-amplify\";\nimport { mapGetters } from \"vuex\";\nvar config = require(\"../config.js\");\n\nexport default {\n  name: \"app\",\n  data() {\n    return {\n      friendsWatching: [],\n      confirmedEvents: [],\n      pendingEvents: [],\n      sizeConfirmedEvents: -1,\n      sizePendingEvents: -1,\n      selectedCard: -1,\n      result: [],\n      showLoading: true,\n      size: -1,\n      form: {\n        description: \"\",\n        time: \"\",\n        inviteList: \"\",\n        inviteUrl: \"\",\n      },\n    };\n  },\n  mounted() {\n    this.loadSocial(\"\");\n  },\n  components: {\n    Carousel,\n    Slide,\n    Spinner,\n  },\n  computed: {\n    ...mapGetters({\n      user: \"auth/user\",\n    }),\n  },\n  methods: {\n    onSubmit(event) {\n      event.preventDefault();\n      axios({\n        method: \"POST\",\n        url: config.api.invokeUrl + \"/social/eventcreate\",\n        headers: {\n          Authorization: this.$store.state.auth.token,\n        },\n        data: {\n          creator: this.$store.state.auth.user.attributes.email,\n          description: this.form.description,\n          invitee: this.form.inviteList,\n          time: this.form.time,\n          url: this.form.inviteUrl,\n        },\n      })\n        .then((response) => {\n          // handle success\n          console.log(response[\"data\"]);\n          if (response[\"data\"] == \"Event created successfully\") {\n                      this.showModal(\"event_created\");\n                    }\n          this.form.description = \"\";\n          this.form.time = \"\";\n          this.form.inviteList = \"\";\n          this.form.inviteUrl = \"\";\n          this.loadSocial(\"\")\n        })\n        .catch(function (error) {\n          alert(\"Error Occurred\");\n          console.error(\"Error: \", error);\n        });\n    },\n    onReset(event) {\n      event.preventDefault();\n      this.form.description = \"\";\n      this.form.time = \"\";\n      this.form.inviteList = \"\";\n      this.form.inviteUrl = \"\";\n      // Trick to reset/clear native browser form validation state\n      this.show = false;\n      this.$nextTick(() => {\n        this.show = true;\n      });\n    },\n    eventAccept: function (event) {\n      axios({\n        method: \"POST\",\n        url: config.api.invokeUrl + \"/social/eventstatus\",\n        headers: {\n          Authorization: this.$store.state.auth.token,\n        },\n        data: {\n          eventid: event.eventid,\n          status: \"accepted\",\n        },\n      })\n        .then((response) => {\n          // handle success\n          console.log(response[\"data\"]);\n          if (response[\"data\"] == \"Event updated successfully.\") {\n            this.showModal(\"event_accepted\");\n          }\n          this.loadSocial(\"\");\n        })\n        .catch(function (error) {\n          // handle error\n          alert(\"Error Occurred\");\n          console.error(\"Error: \", error);\n        });\n    },\n    eventReject: function (event) {\n      axios({\n        method: \"POST\",\n        url: config.api.invokeUrl + \"/social/eventstatus\",\n        headers: {\n          Authorization: this.$store.state.auth.token,\n        },\n        data: {\n          eventid: event.eventid,\n          status: \"rejected\",\n        },\n      })\n        .then((response) => {\n          // handle success\n          console.log(response[\"data\"]);\n          if (response[\"data\"] == \"Event updated successfully.\") {\n            this.showModal(\"event_declined\");\n          }\n          this.loadSocial(\"\");\n        })\n        .catch(function (error) {\n          // handle error\n          console.error(\"Error: \", error);\n        });\n    },\n\n    hoverCard(selectedIndex) {\n      this.selectedCard = selectedIndex;\n    },\n    isSelected(cardIndex) {\n      return this.selectedCard === cardIndex;\n    },\n    async loadSocial(caller) {\n      this.showLoading = true;\n      await Auth.currentSession().then((data) =>\n        axios({\n          method: \"GET\",\n          url: config.api.invokeUrl + \"/social\",\n          headers: {\n            Authorization: data.idToken.jwtToken,\n          },\n          params: {\n            userid: this.$store.state.auth.user.username,\n            useremail: this.$store.state.auth.user.attributes.email,\n          },\n        })\n          .then((response) => {\n            console.log(response);\n            // handle success\n            this.friendsWatching = response[\"data\"][\"friend_watchlist\"];\n            this.pendingEvents = response[\"data\"][\"pending_events\"];\n            this.confirmedEvents = response[\"data\"][\"confirmed_events\"];\n            this.size = this.friendsWatching.length;\n            this.sizePendingEvents = this.pendingEvents.length;\n            this.sizeConfirmedEvents = 